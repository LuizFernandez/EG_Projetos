	"z = 4" -> "while (z == 4)"
	"while (z == 4)"
	"while (z == 4)" -> "int z = 1" [label=false]
	fim
	"int z = 1"
	"int z = 1" -> fim
	"while (z == 2)" [shape=diamond]
	"while (z == 3)" [shape=diamond]
	"while (z == 4)" [shape=diamond]
