digraph {
	"int z = 0"
	inicio
	inicio -> "int z = 0"
	"if (z == 1)"
	"int z = 0"
	"int z = 0" -> "if (z == 1)"
	"if (z == 1)"
	"if (z == 1)" -> "z = 2" [label=true]
	"z = z + 1"
	"z = 2"
	"z = 2" -> "z = z + 1"
	fim
	"z = z + 1"
	"z = z + 1" -> fim
	"if (z == 1)"
	"if (z == 1)" -> fim [label=false]
	"if (z == 1)" [shape=diamond]
	"if (z == 1)" [shape=diamond]
}
