digraph {
	"int z = 5"
	inicio
	inicio -> "int z = 5"
	"int b = 0"
	"int z = 5"
	"int z = 5" -> "int b = 0"
	"while(z < 5)"
	"int b = 0"
	"int b = 0" -> "while(z < 5)"
	"while(z < 5)"
	"while(z < 5)" -> "z = z - 1" [label=true]
	"b = b + 1"
	"z = z - 1"
	"z = z - 1" -> "b = b + 1"
	"while(z < 5)"
	"b = b + 1"
	"b = b + 1" -> "while(z < 5)"
	"while(z < 5)"
	"while(z < 5)" -> "int c = b" [label=false]
	fim
	"int c = b"
	"int c = b" -> fim
	"while(z < 5)" [shape=diamond]
}
